{
    "name": "santran/bitmex",
    "description": "PHP BITMEX API is an asynchronous PHP library for the Bitmex API designed to be easy to use.",
    "keywords": ["bitmex", "cryptocurrency", "WebSocket", "trading", "api"],
    "license": "MIT",
    "authors": [
        {
            "name": "San Tran",
            "email": "santran686@gmail.com"
        }
    ],
    "repositories": [{
            "type": "vcs",
            "url": "https://github.com/mrsantran/bitmex"
        }],
    "require": {
        "php": ">=5.4",
        "ratchet/pawl": "^0.3.0",
        "react/socket": "^1.0 || ^0.8 || ^0.7",
        "ratchet/rfc6455": "^0.2.3",
        "guzzlehttp/guzzle": "^6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8"
    },
    "config": {
        "optimize-autoloader": true,
        "minimum-stability": "dev"
    },
    "autoload": {
        "classmap": [
            "bitmex.php"
        ]
    }
}
